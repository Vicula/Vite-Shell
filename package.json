{
  "name": "vite-shell",
  "version": "0.1.0",
  "private": true,
  "main": "vite.config.js",
  "author": "@Vicula",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vicula/Vite-Shell"
  },
  "keywords": [
    "shopify",
    "theme",
    "vite",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint:markup": "vti diagnostics",
    "lint": "yarn lint:script && yarn lint:style && yarn lint:markup",
    "preinstall": "node scripts/checkYarn.js",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "node scripts/verifyCommits.js"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@shopify/themekit": "^1.1.4",
    "@types/node": "^14.14.22",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vitejs/plugin-vue-jsx": "^1.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "accoutrement": "^2.1.3",
    "chalk": "^4.1.0",
    "eslint": "^7.19.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "stylelint": "^13.9.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.65",
    "vti": "^0.0.24"
  }
}
